{
    "name": "ajithnow/laravel-subscription-webhooks",
    "description": "Laravel package for handling Apple and Google subscription webhooks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ajithkumar S",
            "email": "me@ajithnow.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "psr/log": "^3.0",
        "guzzlehttp/guzzle": "^7.9",
        "firebase/php-jwt": "^6.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "SubscriptionWebhooks\\Laravel\\": "src/Laravel/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SubscriptionWebhooks\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SubscriptionWebhooks\\Laravel\\SubscriptionWebhookServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
